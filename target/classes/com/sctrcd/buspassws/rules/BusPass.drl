package com.sctrcd.busspassws.rules

import com.sctrcd.buspassws.facts.*;


declare IsOk
	message: Message
end
declare IsBad
	message: Message
end
declare IsTRNSCCFUL100
	message: Message
end
declare IsTRNSCCFUL200
	message: Message
end
declare IsTRNSCCFUL300
	message: Message
end

rule IsBadPolicy when
    p : Message( id < 16 )
then
    insertLogical( new BadDecision( p ) );
end

rule IsOkPolicy when
    p : Message( id >= 16 )
then
    insertLogical( new OkDecision( p )  );
end
